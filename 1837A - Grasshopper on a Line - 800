#include <bits/stdc++.h>
using namespace std;

int main() 
{
	int t;
	cin>>t;
	while(t--)
	{
	    int x, k;
	    cin>>x>>k;
	    
	    int ans=0;
	    
	    if(x%k!=0)
	        cout<<1<<endl<<x<<endl;
	    else
	    {
	        for(int i=x-k; i>0; i--)
	        {
	            if(i%2!=0)
	            {
	                ans=i;
	                break;
	            }
	        }
	        cout<<2<<endl<<ans<<" "<<x-ans<<endl;
	    }
	    
	}
    return 0;
}
